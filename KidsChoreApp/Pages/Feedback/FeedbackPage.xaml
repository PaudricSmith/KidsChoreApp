<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KidsChoreApp.Pages.Feedback.FeedbackPage"
             Title="Feedback">
    
    <StackLayout Padding="20">
        <Label Text="What do you think about this App?" FontSize="Large" HorizontalOptions="Center"/>
        <Button Text="I love this App!" Clicked="OnLoveAppClicked"/>
        <Button Text="I don't like the App!" Clicked="OnDislikeAppClicked"/>
    </StackLayout>
    
</ContentPage>
